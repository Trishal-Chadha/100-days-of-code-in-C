# Birthday Wishing Program for Rohan (Jija Ji)

def happy_birthday(name="Rohan"):
    print("\n🎉🎂 Happy Birthday,", name, "! 🎂🎉\n")
    print("May your life run without bugs,")
    print("your happiness always overflow,")
    print("and your success compile perfectly! 🚀\n")

    # ASCII Cake
    print("      ,,,,,,")
    print("     ||||||||")
    print("     ||||||||   🎂")
    print("     ||||||||")
    print("     `''''''`")
    print("   Happy Birthday,", name, "!")
    print("   -- From Bhidolhi with love ❤️")

happy_birthday("Rohan")
